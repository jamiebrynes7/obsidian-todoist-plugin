{
  "name": "obsidian-todoist-plugin",
  "version": "2.0.1",
  "description": "A Todoist plugin for Obsidian",
  "scripts": {
    "check": "tsc --noEmit",
    "dev": "npm run check && VITE_ENV=dev vite build",
    "build": "vite build",
    "test": "vitest",
