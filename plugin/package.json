    "framer-motion": "^11.1.8",
    "obsidian": "1.7.2",
    "react": "^18.2.0",
    "react-aria-components": "^1.1.1",
    "react-dom": "^18.2.0",
    "react-textarea-autosize": "^8.5.3",
    "snakify-ts": "^2.3.0",
    "tslib": "^2.4.1",
    "yaml": "^2.1.3",
    "zod": "^3.24.1",
    "zustand": "^4.5.2"
